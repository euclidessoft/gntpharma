{% extends 'admin.html.twig' %}
{% block title %} {{ "Commande"|trans }} - GNT Pharma {% endblock %}
{% block finance %}active{% endblock %}
{% block transfert %}active{% endblock %}
{% block caisse_banque %}active{% endblock %}

{% block body %}
<div class="col-lg-4 offset-lg-2 mt-4 fond-7 pb-5 pt-3 px-5">
    <h4 class="mb-3">
        <i class="fa-brands fa-product-hunt mr-2"></i><i class="fa fa-plus mr-2"></i>
        {{ 'Transfert Caisse vers Banque'|trans }}
    </h4>
    {{ include('transfert/_form.html.twig') }}
</div>
    <div class="col-md-3 offset-md-3 mt-4 pt-3 fond-7">
        <div class="centrer">
            {#                <div class="row">#}
            <button type="button" class="btn btn-shadow btn-green btn-normal mb-3 mr-3" id="send"
                    onclick="envoie()">
                <i class="fa fa-save"></i>
                {{ 'Enregistrer'|trans }}
            </button>

            <button type="reset" class="btn btn-shadow btn-danger btn-normal mb-3">
                <i class="fa fa-delete-left"></i>
                {{ 'Effacer'|trans }}
            </button>
            {{ form_rest(form) }}
            {#                </div>#}
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}
