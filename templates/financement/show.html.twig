<!DOCTYPE html>

<title>Financement</title>

{% block body %}
    <h1>Financement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ financement.id }}</td>
            </tr>
            <tr>
                <th>Provenance</th>
                <td>{{ financement.provenance }}</td>
            </tr>
            <tr>
                <th>Motif</th>
                <td>{{ financement.motif }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ financement.montant }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ financement.date ? financement.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ financement.Type }}</td>
            </tr>
            <tr>
                <th>Banque</th>
                <td>{{ financement.banque }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ financement.numero }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('financement_index') }}">back to list</a>

    <a href="{{ path('financement_edit', {'id': financement.id}) }}">edit</a>

    {{ include('financement/_delete_form.html.twig') }}
{% endblock %}
