<form method="post" action="{{ path('banque_delete', {'id': banque.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette banque');" class="d-inline-block">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ banque.id) }}">
    <button class="btn btn-danger"><i class="fas fa-trash"></i></button>
</form>
